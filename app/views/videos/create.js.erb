<% debugger %>
$(document).trigger("hide-popovers");
<% if @video.new_record? %>
	$.alert('<%= error_messages_for(@video) %>', 'error');
<% else %>
	$(document).trigger("reset-form");
	$("body").load('<%= edit_video_path(@video) %>');
<% end %>

